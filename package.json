    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "storybook": "^7.4.6",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
